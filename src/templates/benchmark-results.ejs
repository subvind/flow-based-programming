<div id="benchmark-results">
  <h2>Benchmark Results</h2>
  <table>
    <thead>
      <tr>
        <th>Message Size</th>
        <th>Message Count</th>
        <th>Average Processing Time</th>
        <th>Messages per Second</th>
      </tr>
    </thead>
    <tbody>
      <% 
        let count = 0;
        let totalMessagesPerSecond = 0;
        for (const [size, data] of Object.entries(results)) { 
          count++;
          totalMessagesPerSecond += parseFloat(data.messagesPerSecond);
      %>
        <tr>
          <td><%= size %> bytes</td>
          <td><%= data.messageCount %></td>
          <td><%= data.averageProcessingTime.toFixed(2) %> ms</td>
          <td><%= data.messagesPerSecond %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
  <% 
    const totalAverageMPS = (totalMessagesPerSecond / count).toFixed(2);
  %>
  <p>Total Average: <strong><%= totalAverageMPS %></strong> messages/second</p>
</div>